{
  "name": "denali-cli",
  "version": "0.0.12",
  "description": "The official CLI for starting, building, and running Denali apps and addons",
  "homepage": "http://denali.js.org",
  "repository": "denali-js/denali-cli",
  "author": {
    "name": "Dave Wasmer",
    "email": "davewasmer@gmail.com",
    "url": "davewasmer.com"
  },
  "bin": {
    "denali": "dist/bin/denali"
  },
  "main": "dist/lib/index.js",
  "engines": {
    "node": ">=6.9.0"
  },
  "keywords": [
    "denali",
    "cli",
    "framework",
    "opinionated",
    "server"
  ],
  "devDependencies": {
    "@types/chalk": "^0.4.31",
    "ava": "^0.20.0",
    "nyc": "^11.0.3",
    "tslint": "^5.4.3",
    "typescript": "^2.4.1"
  },
  "scripts": {
    "build": "rm -rf dist && tsc && mv dist/bin/denali.js dist/bin/denali && chmod +x dist/bin/denali && echo 'Build successful'",
    "prepublish": "in-publish && npm run build || not-in-publish",
    "lint": "! tslint --project tsconfig.json -c tslint.json --type-check | sed \"s|$PWD/||\" | grep ERROR",
    "test": "npm run lint && rm -rf dist coverage .nyc_output && npm run build && nyc ava dist/test/acceptance"
  },
  "license": "MIT",
